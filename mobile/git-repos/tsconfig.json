{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src/",
    "paths": {
      "~/*": ["src/*"],
      "@screens/*": ["screens/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"],
      "@store/*": ["store/*"],
      "@themes/*": ["themes/*"],
      "@layout/*": ["layout/*"],
      "@services": ["services"]
    }
  },
  "exclude": ["node_modules", "android", "ios"]
}
